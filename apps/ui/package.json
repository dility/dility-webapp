{
  "name": "ui",
  "version": "0.0.1",
  "description": "dility.io frontend webapplication",
  "productName": "dility frontend application",
  "author": "avi.mehenwal@dility.io",
  "private": true,
  "scripts": {
    "dev": "quasar dev",
    "prod": "quasar build && python3 -m http.server --directory dist/spa 8080",
    "lint": "eslint --ext .js,.ts,.vue ./",
    "ci:build": "docker build -t ghcr.io/dility/ui . && docker scan ghcr.io/dility/ui",
    "ci:serve": "docker run --rm -it -p 9000:80 ghcr.io/dility/ui",
    "format": "prettier --write \"**/*.{js,ts,vue,,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0"
  },
  "dependencies": {
    "@pnpm/make-dedicated-lockfile": "^0.3.20",
    "@quasar/extras": "^1.0.0",
    "postcss": "^8.1.0",
    "quasar": "^2.6.0",
    "vue": "^3.0.0",
    "vue-i18n": "^9.2.0",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@quasar/app-vite": "^1.0.0",
    "@types/node": "^12.20.21",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^9.0.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
